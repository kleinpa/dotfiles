alias cp='cp -i'
alias mv='mv -i'
alias e='$EDITOR'
alias ev='$VISUAL'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias grep='grep --color=auto'
alias less='less -I'
alias ls='ls --color=auto --file-type -h'

alias emacs="emacs -nw"

# Default to -h versions of commands
alias free='free -h'
alias df='df -h'

# tmux aliases
alias tm="tmux"
alias tmr="tmux rename-window"
alias tmain="tmux new-session -A -s main"

alias ibazel="npx @bazel/ibazel"
